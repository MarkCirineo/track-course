{
  "name": "tm-course-map",
  "version": "0.1.3",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env -- next dev --webpack",
    "build": "prisma generate && next build",
    "start": "dotenv -e .env -- next start",
    "lint": "eslint",
    "postinstall": "prisma generate",
    "format": "prettier --write \"src/**/*.{js,ts,jsx,tsx,json,md}\"",
    "orphans": "dotenv -e .env -- npx tsx scripts/find-orphan-courses.ts",
    "orphans:delete": "dotenv -e .env -- npx tsx scripts/find-orphan-courses.ts --delete"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "next-auth": "5.0.0-beta.30",
    "next-themes": "^0.4.6",
    "pg": "^8.18.0",
    "prisma": "^7.3.0",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-leaflet": "^5.0.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "postcss": "^8.4.49",
    "prettier": "^3.8.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5"
  }
}
